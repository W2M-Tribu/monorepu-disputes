{
  "request": {
    "method": "POST",
    "url": "/api/token-check",
    "headers": {
      "Authorization": {
        "equalTo": "Bearer abc123"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "message": "Token is valid"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}